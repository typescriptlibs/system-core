{
    "extends": "./tsconfig-package.json",
    "typedocOptions": {
        "excludeExternals": true,
        "excludeNotExported": true,
        "excludePrivate": true,
        "excludeProtected": true,
        "includeDeclarations": false,
        "mode": "file",
        "name": "System Core Library",
        "plugin": "none",
        "readme": "README.md",
        "theme": "node_modules/typescriptlibs.typedoc.theme"
    }
}
